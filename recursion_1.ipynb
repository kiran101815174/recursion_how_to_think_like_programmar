      "source": [
        "def only_pos(arr,size):\n",
        "  if size==0:\n",
        "    return 0\n",
        "  if arr[size-1]>0:\n",
        "    return arr[size-1]+only_pos(arr,size-1)\n",
        "  return only_pos(arr,size-1)\n",
        "arr=[1,-1,2,-100,3]\n",
        "print(only_pos(arr,5))"
      ],
