      "source": [
        "def target_occ(a,size,target):\n",
        "  if size==0:\n",
        "    return False\n",
        "  if a[size-1]==target:\n",
        "    return 1+ target_occ(a,size-1,target)\n",
        "  return target_occ(a,size-1,target)\n",
        "\n",
        "arr=[1,2,4,5,6,7,2,3,4,2,4,2,4,2]\n",
        "#find occurances of 2\n",
        "print(target_occ(arr,len(arr),2)) "
      ],
